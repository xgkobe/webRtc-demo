    "start": "ts-node ./config/scripts/check-port.ts && npm run start:renderer",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./config/webpacks/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./config/webpacks/webpack.renderer.dev.ts",
    "start:main": "cross-env NODE_ENV=development electron -r ts-node/register/transpile-only ./src/main/main.ts",
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./config/webpacks/webpack.renderer.prod.config.ts",
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\""
